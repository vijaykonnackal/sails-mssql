{
  "name": "sails-sqlserver-adapter",
  "description": "a MsSql adapter for Waterline and Sails.js",
  "version": "1.1.2",
  "author": "Vijay Konnackal <vijay.konnackal@gmail.com>",
  "url": "https://github.com/vijaykonnackal/sails-mssql",
  "keywords": [
    "mssql",
    "orm",
    "waterline",
    "sails"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vijaykonnackal/sails-mssql.git"
  },
  "dependencies": {
    "@sailshq/lodash": "^3.10.4",
    "async": "^3.1.0",
    "flaverr": "^1.10.0",
    "machine": "^15.2.2",
    "machinepack-mssql": "https://github.com/vijaykonnackal/machinepack-mssql.git",
    "waterline-utils": "https://github.com/vijaykonnackal/waterline-utils.git"
  },
  "devDependencies": {
    "benchmark": "^2.1.4",
    "eslint": "^6.6.0",
    "mocha": "^6.2.2",
    "chai": "^4.2.0",
    "waterline-adapter-tests": "^1.0.1",
    "mssql": "^5.1.0",
    "np": "*"
  },
  "scripts": {
    "test": "node ./node_modules/mocha/bin/mocha test/adapter/unit --recursive",
    "fasttest": "node ./node_modules/mocha/bin/mocha test/adapter/unit --recursive && node test/adapter/integration/runner",
    "pretest": "npm run lint",
    "lint": "eslint lib helpers test",
    "benchmark": "node ./node_modules/mocha/bin/mocha test/benchmarks --recursive",
    "release": "np"
  },
  "main": "lib/adapter",
  "directories": {
    "lib": "./lib"
  },
  "license": "MIT",
  "bugs": "https://github.com/vijaykonnackal/sails-mssql/issues",
  "waterlineAdapter": {
    "waterlineVersion": "~0.13.0",
    "interfaces": [
      "semantic",
      "queryable",
      "migratable",
      "associations",
      "sql"
    ],
    "features": [
      "crossAdapter",
      "unique",
      "autoIncrement",
      "schemas"
    ]
  }
}
